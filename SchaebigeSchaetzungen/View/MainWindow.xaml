<Window x:Class="SchaebigeSchaetzungen.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchaebigeSchaetzungen.View"
        xmlns:views="clr-namespace:SchaebigeSchaetzungen.View"
        xmlns:vms="clr-namespace:SchaebigeSchaetzungen.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Resources>
            <DataTemplate DataType="{x:Type vms:RegistrationVM}">
                <views:Registration/>
            </DataTemplate>
        </Grid.Resources>
        <views:Registration DataContext="{Binding CurrentViewModels}" />
        </Grid>
</Window>
